<view class="searchview"><view class="search"><icon class="icon-class" size="12px" type="search"></icon><input class="input" style="padding-left:12rpx;" placeholder="{{inputHint}}" type="{{inputType}}" password="{{isPassword}}" confirm-type="{{confirmType}}" data-event-opts="{{[['confirm',[['inputConfirm',['$event']]]],['input',[['inputListener',['$event']]]]]}}" value="{{inputValue}}" bindconfirm="__e" bindinput="__e"/><icon class="{{[isClearShow?'clearImgShow':'clearImgHide']}}" size="16px" type="clear" data-event-opts="{{[['tap',[['clearTap',['$event']]]]]}}" bindtap="__e"></icon></view><block wx:if="{{resultList.length!=0}}"><scroll-view class="resultview" scroll-y><block wx:for="{{resultList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['bindNavigate',['$0'],[[['resultList','',index,'name']]]]]]]}}" class="resultitem" bindtap="__e"><icon class="icon-class" size="12px" type="search"></icon>{{''+item.name+''}}</view></block></scroll-view></block></view>